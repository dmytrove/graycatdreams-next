{
  "name": "graycatdreams-next",
  "version": "0.2.1",
  "private": true,
  "description": "Create beautiful 3D animations with your images using WebGL and Three.js",
  "keywords": [
    "3D animation",
    "image gallery",
    "WebGL",
    "Three.js",
    "interactive art"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next out node_modules/.cache",
    "postinstall": "npm rebuild sharp"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@react-three/fiber": "^9.1.2",
    "@vercel/speed-insights": "^1.2.0",
    "cookie": "^1.0.2",
    "multer": "^2.0.0",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "sharp": "^0.34.2",
    "three": "^0.176.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/three": "^0.176.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "typescript": "^5",
    "wrangler": "^4.16.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
